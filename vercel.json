{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }
  